(function(){var w=window;var o=w.ysm_customData;if(typeof(o)!="object"||o===null){o=w.pm_customData}var adCenterIds=['1305469','1305466'];var segmentIds=['1K1HT794PTGGIS','1B4KES55K756MM','11JQ0J0K6THTAE','1S2IKDLL4679IE','11FUDT1E4DTJ0A','1HFFMK3DNUH3HQ','124KF9N2IDLQ5A','1MLF5N60K4UT72','1IGNJ3408ME2NE','1RL5JCIQ8PU6BC','1P1U9HQ0MSIO8I','1277NAS84OSN5S','1KN9CE8Q4UL036','16I7F0CMBD5976','1RJAVI1KRAH8PA','1GGHJ1K1UQ7ABC','1VFEDEP397IHEI','1O2N9BDKKBBV1I','1QHQ58MR6SCJHO','1E6KMA66DTB8OK'];var segId=-1;var evtId=-1;var bf=0;var amount="";if(typeof(w.ysm_bf)!="undefined"){bf=w.ysm_bf}if(typeof(o)=="object"&&o!==null){for(var d in o){if(typeof(d)=="string"&&d!==null&&typeof(o[d])=="string"&&o[d]!==null&&o[d].length>0){if(d=="conversion"){segId=0;evtId=1}else{if(d=="segment"){segId=0}else{if(d.indexOf("segment_")!=-1){segId=0;for(var i=0;i<segmentIds.length;i++){if(d.indexOf("segment_"+segmentIds[i])!=-1){segId=i;break}}}}}var a=o[d].split(",");for(var j=0;j<a.length;j++){if(a[j].indexOf("name=conversion")!=-1||a[j].indexOf("event=1")!=-1){evtId=1}else{if(a[j].indexOf("name=shopper")!=-1||a[j].indexOf("event=0")!=-1){evtId=0}}var nv=a[j].split("=");if(nv.length>1){var n=nv[0].toLowerCase();var v=nv[1];if(n.indexOf("amount")!=-1){amount=v}}}if(segId!=-1){break}}}}if(!window.mstag_imgs){window.mstag_imgs=[]}var ystage=(evtId!=-1?""+(evtId+3):(bf==1?"2":"0"));var ygoal=(segId!=-1?""+segId:"0");for(var aId in adCenterIds){if(adCenterIds.hasOwnProperty(aId)){var img=new Image();img.src="//"+adCenterIds[aId]+".r.msn.com/?type=1&dedup=1&piggyback=Y&domainID="+adCenterIds[aId]+"&YGoal="+ygoal+"&YStage="+ystage+"&Revenue="+amount;window.mstag_imgs.push(img)}}})();